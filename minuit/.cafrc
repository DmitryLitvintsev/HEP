[global]
; Global definitions
;
; List of available analysis farms and default choice
af_list=caf,knu-mc-only,as-mc-only,cnaf,ucsd-mc-only,cafcondor,sdsccaf,torcaf2
default_af=caf


[caf]
; CDF Central Analysis Farm
;
; Headnode name, submitter port, and service/host principal
host=fcdfhead1.fnal.gov
submitter_port=8000
monitor_port=8020
groupupdate_port=8040
; icaf_host=cdfhead1.fnal.gov
icaf_port=9035
svc=host
;
; List of available process types and default choice
pt_list=short,medium,long,test,cdfmc,cdfopr,trigsim,sam,short_italy,long_cmu,long_pitt,long_mit,long_uk,long_yale,long_jhu,long_harvard,long_ucdavis,long_japan,long_geneva,long_ttu,long_michigan,physmon
default_pt=short


